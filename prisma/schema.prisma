generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id   Int    @id @default(autoincrement())
  name String
  Wpp String 
  product Product[] 
}

model Product { 

  id Int @id @default(autoincrement())
  product String
  pg String
  idBuyer Int
  buyer User @relation(fields: [idBuyer], references: [id])

} 
